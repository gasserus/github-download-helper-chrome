{
  "moduleFileExtensions": [
    "ts",
    "js"
  ],
  "transform": {
    "^.+\\.(ts|tsx)$": "ts-jest"
  },
  "testMatch": [
    "**/test/**/*.test.ts"
  ],
  "testEnvironment": "node",
  "setupFiles": [
    "<rootDir>/jest-setup.config.ts"
  ],
  "setupFilesAfterEnv": ["./test/setupTests.ts"],
  "collectCoverageFrom": [
    "./src/!(model)/**/*.ts"
  ]
}
